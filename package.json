{
  "name": "@persistr/clif-plugin-persistr",
  "version": "1.1.0",
  "description": "Persistr plugin for @persistr/clif",
  "main": "index.js",
  "author": "Sasa Djolic <sasa.djolic@gmail.com> (https://www.linkedin.com/in/sasadjolic/)",
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/persistr/clif-plugin-persistr.git"
  },
  "dependencies": {
    "@persistr/js": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "babelify": "^10.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "plugins": [
            "@babel/plugin-proposal-nullish-coalescing-operator",
            "@babel/plugin-proposal-optional-chaining"
          ]
        }
      ]
    ]
  }
}
